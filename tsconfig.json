{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "typeRoots": [
      "./node_modules/",
      "./node_modules/@types/",
      "./types"
    ],
    "types": [
      "vite/client",
      "vite-plugin-svg-icons-ssr/client"
    ],
    "paths": {
      "~/*": ["./components/*"],
      "@/*": ["./*"],
      "#/*": ["./types/*"]
    },
    "jsx": "preserve",
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "exclude": [
    "dist"
  ]
}